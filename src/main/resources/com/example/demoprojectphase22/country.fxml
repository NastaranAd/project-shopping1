<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="399.0" prefWidth="336.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demoprojectphase22.Country">
   <children>
      <TextField fx:id="country" layoutX="94.0" layoutY="162.0" promptText="country name " />
      <Button fx:id="search" layoutX="66.0" layoutY="224.0" mnemonicParsing="false" onMouseClicked="#searchButton" text="search">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="backButton" layoutX="227.0" layoutY="224.0" mnemonicParsing="false" onMouseClicked="#backButton" text="back">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font>
      </Button>
      <ImageView fitHeight="97.0" fitWidth="165.0" layoutX="99.0" layoutY="30.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@country1.jpg" />
         </image>
      </ImageView>
      <TextArea fx:id="itemDescription" layoutX="88.0" layoutY="272.0" prefHeight="79.0" prefWidth="161.0" />
   </children>
</AnchorPane>
